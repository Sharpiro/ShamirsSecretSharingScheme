{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "preLaunchTask": "make",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/c/gitbase/ShamirsSecretSharingScheme/build/src/Shamir-cli",
            "args": [
                "disthex",
                "3",
                "2"
            ],
            "stopAtEntry": false,
            "cwd": "/mnt/c/gitbase/ShamirsSecretSharingScheme",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "pipeTransport": {
                "pipeCwd": "",
                "pipeProgram": "c:\\windows\\sysnative\\bash.exe",
                "pipeArgs": [
                    "-c"
                ],
                "debuggerPath": "/usr/bin/gdb"
            },
            "sourceFileMap": {
                "/mnt/c": "c:\\"
            }
        }
    ]
}